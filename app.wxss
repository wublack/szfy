/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.vertical-content{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.base-title {
  background-color: #f5f5f5;
  height: 75rpx;
  align-items: center;
  display: flex;
}

.base-title-line {
  background-color: #1472f3;
  width: 4rpx;
  height: 30rpx;
  margin-left: 30rpx;
  margin-right: 10rpx;
}

.base-title-txt {
  font-size: 30rpx;
  color: #333;
}

.back-btn{
  margin-left: 30rpx;
  font-size: 45rpx;
  color: #999;
}

.middle-title{
  font-size: 32rpx;
  color: #000;
  font-weight: bold;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

